{
  "name": "unleash",
  "version": "1.0.0",
  "description": "Web app >>> desktop app",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "build:package": "pkg ./package.json -t node12-win -o dist/unleash/unleash_tmp.exe",
    "build:nodew": "create-nodew-exe dist/unleash/unleash_tmp.exe dist/unleash/unleash.exe",
    "build:finalize": "rimraf dist/unleash/unleash_tmp.exe",
    "build": "run-s build:package build:nodew build:finalize"
  },
  "bin": {
    "unleash": "./index.js"
  },
  "pkg": {
    "scripts": "*.js"
  },
  "author": "sAbIz",
  "license": "Apache-2.0",
  "dependencies": {
    "carlo": "^0.9.46",
    "json5": "^2.1.1"
  },
  "devDependencies": {
    "create-nodew-exe": "^1.0.5",
    "npm-run-all": "^4.1.5",
    "pkg": "^4.4.2",
    "rimraf": "^3.0.0"
  }
}
